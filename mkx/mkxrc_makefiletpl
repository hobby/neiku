# mkx makefile template
# vim:ts=4:sw=4:noexpandtab

# include origin makefile's config
include {_origin_makefile_}

# new rule in mkx makefile
%.o:%.c
	${CC} -c $< ${CCFLAGS} ${INCLUDES} -o $@

%.o:%.cpp
	${CXX} -c $< ${CXXFLAGS} ${INCLUDES} -o $@

%.o:%.cc
	${CXX} -c $< ${CXXFLAGS} ${INCLUDES} -o $@
